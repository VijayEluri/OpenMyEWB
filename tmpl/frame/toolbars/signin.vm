<form action="$base/actions/DoSignIn" method="post" enctype="multipart/form-data" name="app_form">
<input type="hidden" name="action" value="save"/>
<input type="hidden" name="targetURL" value="$targetURL"/>

<div style="margin-bottom: 5px;">
<label for="formelemUsername" class="fieldname  ">Email Address:</label>
<br />
<input type="text" name="Username" value="" style="width: 150px;" id="formelemUsername"/>
</div>

<div style="margin-bottom: 5px;">
<label for="formelemPassword" class="fieldname  ">Password:</label>
<br />					
<input type="password" name="Password" style="width: 150px;" id="formelemPassword"/>	
</div>

								
<div style="padding-left: 45px;margin-bottom: 5px;">
<input type="submit" value="sign in" class="button" />
</div>
</form>

<div style="padding-left: 15px;">
<a href="$base/profile/ForgotPassword" class="more">Forgot your password?</a>	
</div>	
