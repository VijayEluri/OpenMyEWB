<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.myewb.model">

    <class name="TicketOrderModel" table="ticketorders">

        <id name="id" type="int" unsaved-value="0">
            <generator class="native"/>
        </id>
        
        
	<property name="name" type="string"/>
	<property name="email" type="string"/>
	<property name="phone" type="string"/>
	<property name="language" type="string"/>
	<property name="needs" type="string"/>
	
	<property name="address1" type="string"/>
	<property name="suite" type="string"/>
	<property name="address2" type="string"/>
	<property name="city" type="string"/>
	<property name="province" type="string"/>
	<property name="postalcode" type="string"/>
	<property name="country" type="string"/>
	
	<property name="receiptNum" type="string"/>
	<property name="date" type="timestamp"/>
	<property name="numtickets" type="int"/>
	
        
        


    </class>

</hibernate-mapping>
